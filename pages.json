{
	"easycom": {
			"autoscan": true,
			"custom": {
				// uni-ui 规则如下配置
				"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
			}
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "YINCE"
			}
		},
		{
			"path": "pages/form/form",
			"style": {
				"navigationBarTitleText": "创建小组"
			}
		},
		{
			"path": "pages/list/list",
			"style": {
				"navigationBarTitleText": "加入小组"
			}
		},
		{
			"path": "pages/group/group",
			"style": {
				"navigationBarTitleText": "我的小组"
			}
		},
		{
			"path": "pages/tip/tip",
			"style": {
				"navigationBarTitleText": "其他小组"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "YICNCE",
		"navigationBarBackgroundColor": "#c1380f",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}